{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
